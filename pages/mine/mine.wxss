page {
    background-color: #f1f1f4
}

#minePage .top {
    width: 100%;
    overflow: hidden;
    min-height: 34px;
    font-size: 18px;
    color: #fff;
    background-color: #ff4a4a
}

#minePage .top .md {
    overflow: hidden;
    margin: 15px 0;
    height: 65px
}

#minePage .top .md .rightInfo {
    margin-left: 8px;
    margin-top: 3px;
    position: relative;
    width: 73%
}

#minePage .top .md .rightInfo .name {
    line-height: 30px;
    font-size: 18px;
    margin-left:80px;
    color: #fff
}

#minePage .top .md .rightInfo .no {
    font-size: 28rpx;
    background: url("img/phone.png") no-repeat;
    background-size: 12px 18px;
    background-position: left center;
    line-height: 30px;
    padding-left: 80px;
    color: #fff
}

#minePage .top .md .rightInfo .jt {
    width: 20px;
    position: absolute;
    top: 0;
    right: 0;
    line-height: 60px;
    padding-right: 20px;
    text-align: right
}

#minePage .top .md .rightInfo .jt>image {
    width: 8px;
    height: 14px;
    -moz-transform: rotate(180deg);
    -o-transform: rotate(180deg);
    -webkit-transform: rotate(180deg);
    vertical-align: middle
}

#minePage .top .loadingImg {
    width: 62px;
    height: 62px;
    margin-left: 15px;
    border-radius: 50px;
    border: 1px solid #FFF;
    background-size: 60px;
    float: left;
    text-align: center
}

#minePage .tabInfo {
    overflow: auto;
    padding: 0 !important
}

#minePage .tabInfo .jewel {
    font-size: 14px;
    padding-left: 15px;
    overflow: hidden;
    color: #ff4a4a;
    background-color: #fff;
    position: relative;
    border-bottom: 1px solid #e8e6ed;
    line-height: 60px
}

#minePage .tabInfo .jewel .myPacketNum {
    position: absolute;
    height: 60px;
    width: 100%
}

#minePage .tabInfo .jewel .myPacketNum .num {
    margin: 0 auto;
    padding: 0;
    width: 270px;
    text-align: center;
    height: 60px
}

#minePage .tabInfo .jewel .myPacketNum .num .jwelAmt {
    font-size: 42px;
    text-align: center
}

#minePage .tabInfo .jewel .myPacketNum .num image {
    width: 35px;
    height: 35px;
    line-height: 61px;
    margin-top: -20px
}

#minePage .tabInfo .jewel .jwelName {
    float: left;
    font-size: 14px
}

#minePage .tabInfo .jewel .jwelImg {
    width: 20px;
    height: 100%;
