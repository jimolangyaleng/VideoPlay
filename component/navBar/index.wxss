/* component/navBar/index.wxss */
.nav{
  width: 100%;
  overflow: hidden;
  position: relative;
  top: 0;
  left: 0;
  z-index: 10;
}
.nav-title{
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  text-align: center;
  position: absolute;
  bottom: 0;
  left: 0;
  z-index: 10;
  font-family:PingFang-SC-Medium;
  font-size:36rpx;
  letter-spacing:4rpx;
}
.nav .back{
  width: 40rpx;
  height: 40rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  padding: 20rpx 30rpx;
}
.bg-white{
  background-color: #ffffff;
}
.bg-gray{
  background-color: #efefef;
}
.overflow{
  overflow: auto;
}
.hidden{
  overflow: hidden;
}